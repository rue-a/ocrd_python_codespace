{
    "image": "mcr.microsoft.com/devcontainers/python",
    "hostRequirements": {
        "cpus": 1
    },
    "waitFor": "onCreateCommand",
    "postCreateCommand": "pip install ocrd",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "notebooks/test.ipynb"
            ]
        },
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter",
                "ms-python.python",
                "mechatroner.rainbow-csv",
                "streetsidesoftware.code-spell-checker"
            ],
            "settings": {}
        }
    }
}