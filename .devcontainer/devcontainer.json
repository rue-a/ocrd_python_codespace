{
    "image": "mcr.microsoft.com/devcontainers/python:1-3.8-bullseye",
    "hostRequirements": {
        "cpus": 1
    },
    "waitFor": "onCreateCommand",
    "postCreateCommand": "sh .devcontainer/setup.sh",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        },
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter",
                "ms-python.python"
            ],
            "settings": {
                "notebook.output.textLineLimit": 100

            }
        }
    }
}